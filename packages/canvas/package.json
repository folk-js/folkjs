{
  "name": "@folkjs/canvas",
  "version": "0.0.1",
  "description": "FolkJS Canvas ",
  "keywords": [
    "matrix",
    "matrices",
    "vector"
  ],
  "author": "Christopher Shank <chris.shank.23@gmail.com>",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    },
    "./reactive-element": {
      "import": "./dist/reactive-element/index.js",
      "types": "./dist/reactive-element/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "wireit",
    "test": "node --experimental-strip-types --test './__tests__/*.test.ts'"
  },
  "wireit": {
    "build": {
      "command": "tsc --build --pretty",
      "clean": "if-file-deleted",
      "files": [
        "src/**",
        "package.json",
        "tsconfig.json"
      ],
      "output": [
        "dist/**",
        "tsconfig.tsbuildinfo"
      ]
    }
  },
  "devDependencies": {
    "expect": "^29.7.0",
    "mitata": "^1.0.34",
    "typescript": "^5.8.2"
  }
}
