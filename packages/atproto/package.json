{
  "name": "@folkjs/atproto",
  "version": "0.0.8",
  "description": "Malleable ATProto explorations",
  "keywords": [
    "DOM"
  ],
  "author": "Christopher Shank <chris.shank.23@gmail.com>",
  "type": "module",
  "license": "MIT",
  "exports": {
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "wireit",
    "test": "wireit",
    "publish": "wireit"
  },
  "wireit": {
    "build": {
      "command": "tsc --build --pretty",
      "clean": "if-file-deleted",
      "dependencies": [
        "../dom:build"
      ],
      "files": [
        "src/**",
        "package.json",
        "tsconfig.json"
      ],
      "output": [
        "dist/**",
        "tsconfig.tsbuildinfo"
      ]
    },
    "test": {
      "command": "node --experimental-strip-types --test './__tests__/*.test.ts'",
      "dependencies": [
        "build"
      ],
      "files": [
        "__tests__/*.test.ts"
      ],
      "output": []
    },
    "publish": {
      "command": "npm publish --access public",
      "dependencies": [
        "build"
      ]
    }
  },
  "dependencies": {
    "@atcute/bluesky-richtext-builder": "^2.0.4",
    "@folkjs/dom": "0.0.8",
    "commonmark": "^0.31.2"
  },
  "devDependencies": {
    "@types/commonmark": "^0.27.10",
    "expect": "^30.2.0",
    "typescript": "^5.8.2"
  }
}
