{
  "name": "@folkjs/labs",
  "version": "0.0.1",
  "description": "Labs",
  "keywords": [
    "labs"
  ],
  "author": "Christopher Shank <chris.shank.23@gmail.com>",
  "type": "module",
  "license": "MIT",
  "exports": {
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    },
    "./standalone/*": {
      "import": "./dist/standalone/*.js",
      "types": "./dist/standalone/*.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "wireit",
    "test": "wireit"
  },
  "wireit": {
    "build": {
      "command": "tsc --build --pretty",
      "clean": "if-file-deleted",
      "dependencies": [
        "../canvas:build",
        "../geometry:build",
        "../propagators:build"
      ],
      "files": [
        "src/**",
        "package.json",
        "tsconfig.json"
      ],
      "output": [
        "dist/**",
        "tsconfig.tsbuildinfo"
      ]
    },
    "test": {
      "command": "node --experimental-strip-types --test './__tests__/*.test.ts'",
      "dependencies": [
        "build"
      ],
      "files": [
        "__tests__/*.test.ts"
      ],
      "output": []
    }
  },
  "dependencies": {
    "@automerge/automerge": "^2.2.8",
    "@automerge/automerge-repo": "^1.2.1",
    "@automerge/automerge-repo-network-websocket": "^1.2.1",
    "@codemirror/lang-javascript": "^6.2.3",
    "@codemirror/view": "^6.36.5",
    "@dimforge/rapier2d": "^0.15.0",
    "@folkjs/canvas": "^0.0.1",
    "@folkjs/geometry": "^0.0.1",
    "@folkjs/propagators": "^0.0.1",
    "@mlc-ai/web-llm": "^0.2.78",
    "codemirror": "^6.0.1",
    "eventemitter3": "^5.0.1",
    "gun": "^0.2020.1240",
    "leaflet": "^1.9.4",
    "peerjs": "^1.5.4",
    "perfect-arrows": "^0.3.7",
    "perfect-freehand": "^1.2.2",
    "recast": "^0.23.11",
    "uhtml": "^4.5.11",
    "webcola": "^3.4.0"
  },
  "devDependencies": {
    "expect": "^29.7.0",
    "mitata": "^1.0.34",
    "typescript": "^5.8.2"
  }
}
